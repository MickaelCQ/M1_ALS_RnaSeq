	keywords = {Animals, DNA, Electric Conductivity, Hydrogen-Ion Concentration, Kidney, Liver, Osmolar Concentration, Phosphates, Proteins, Rats, RNA, Spectrophotometry, Water},
	pages = {474--476, 478--481},
}

