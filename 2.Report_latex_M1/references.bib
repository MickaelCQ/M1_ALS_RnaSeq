@article{singh_normalization_2024,
	title = {Normalization of {RNA}-{Seq} data using adaptive trimmed mean with multi-reference},
	volume = {25},
	copyright = {https://creativecommons.org/licenses/by/4.0/},
	issn = {1467-5463, 1477-4054},
	url = {https://academic.oup.com/bib/article/doi/10.1093/bib/bbae241/7676521},
	doi = {10.1093/bib/bbae241},
	abstract = {The normalization of RNA sequencing data is a primary step for downstream analysis. The most popular method used for the normalization is the trimmed mean of M values (TMM) and DESeq. The TMM tries to trim away extreme log fold changes of the data to normalize the raw read counts based on the remaining non-deferentially expressed genes. However, the major problem with the TMM is that the values of trimming factor M are heuristic. This paper tries to estimate the adaptive value of M in TMM based on Jaeckel’s Estimator, and each sample acts as a reference to find the scale factor of each sample. The presented approach is validated on SEQC, MAQC2, MAQC3, PICKRELL and two simulated datasets with two-group and three-group conditions by varying the percentage of differential expression and the number of replicates. The performance of the present approach is compared with various state-of-theart methods, and it is better in terms of area under the receiver operating characteristic curve and differential expression.},
	language = {en},
	number = {3},
	urldate = {2024-10-21},
	journal = {Briefings in Bioinformatics},
	author = {Singh, Vikas and Kirtipal, Nikhil and Song, Byeongsop and Lee, Sunjae},
	month = mar,
	year = {2024},
	pages = {bbae241},
	file = {PDF:/home/mickael/Zotero/storage/T528DIP8/Singh et al. - 2024 - Normalization of RNA-Seq data using adaptive trimmed mean with multi-reference.pdf:application/pdf},
}

